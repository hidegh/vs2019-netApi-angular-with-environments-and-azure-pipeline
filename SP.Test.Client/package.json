{
  "name": "sp.test",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "ngx": "node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng",
    "start": "npm run ngx serve -- --configuration local",
    "serve:local": "npm run ngx serve -- --configuration local",
    "serve:qa": "npm run ngx serve -- --configuration qa",
    "serve:prod": "npm run ngx serve -- --configuration prod",
    ">>> build-notes": "had to add '-- --configuration local' so that the vs publish call: 'npm run build -- --configuration $(EnvironmentName)' gets executed properly",
